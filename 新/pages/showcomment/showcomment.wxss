.container {
  padding: 0;
  font-size: 30rpx;
  color: #000;
}

.container .feed-item {
  width: 690rpx;
  padding: 30rpx 30rpx 20rpx;
  margin: 7rpx 0 6rpx 0;
  background: #fff;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  box-shadow: 0 2px 5px #eee;
}

.container .feed-item .feed-source {
  width: 690rpx;
  left: 0;
  height: 50rpx;
}

.container .feed-item .feed-source .avatar {
  position: relative;
  display: inline-block;
}

.container .feed-item .feed-source a {
  display: inline-block;
  height: 40rpx;
}

.container .feed-item .feed-source .avatar image {
  /*position: absolute;*/
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50px;
  top: 10rpx;
  vertical-align: middle;
}

.container .feed-item .feed-source .title_style {
  font-size: 27rpx;
  color: #aaa;
  font-weight: lighter;
}

.container .feed-item .answer-body {
  padding: 10rpx 0 0 0;
  /*height: 10rpx;*/
  font-size: 30rpx;
  line-height: 28rpx;
  color: #5b5b5b;
}

.container .feed-item .answer-actions {
  width: 690rpx;
  padding: 10rpx 0 0;
  color: #aaa;
}

.container .feed-item .answer-actions view {
  display: inline-block;
  vertical-align: text-bottom;
  padding: 0 10rpx 0 0;
  font-size: 30rpx;
}
.container .feed-item .answer-actions .details_style{
  font-size: 25rpx;
  color: #aaa;
  font-weight: lighter;
}
.order {
  width: 130rpx;
  height: 130rpx;
  border-radius: 130rpx;
  position: fixed;
  bottom: 30rpx;
  right: 30rpx;
  border: 2px solid #bbb;
  text-align: center;
  background-color: white;
}

.order-icon {
  width: 100rpx;
  height: 100rpx;
  margin-top: 12rpx;
}
